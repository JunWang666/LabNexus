}

/* ---- 全局标签和标题 ---- */
QLabel {
    background-color: transparent;
    color: #1F1F1F;
    font-family: &quot;Segoe UI Variable&quot;, &quot;Microsoft YaHei UI&quot;;
}

/* 主标题，增加一些底部间距 */
QLabel#label_Title {
